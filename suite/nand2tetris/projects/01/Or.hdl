/**
 * Or gate:
 * out = (((a == 1) || (b == 1))), 1, 0) 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=outa);
    Not(in=b, out=outb);
    Nand(a=outa, b=outb, out=out);
}
